Algoritmo "atividade"

Var
idade : inteiro
salario1, salario2, total : real
nome : caractere


Inicio
escreva("Digite seu nome: ")
leia(nome)
escreva("Digite sua idade: ")
leia(idade)
escreval("Digite quanto voce recebeu nos dois ultimos meses:")
leia(salario1)
leia(salario2)
total <- salario1+salario2
escreval("DADOS DIGITADOS:")
escreval("Nome = ", nome)
escreval("Idade = ", idade)
escreval("Salario 1 = ", salario1:7:2)
escreval("Salario 2 = ", salario2:7:2)
escreval("Total = ", total:7:2)

Fimalgoritmo